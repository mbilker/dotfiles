<!DOCTYPE patchbay>
<patchbay version="0.4.5" name="scarlett">
 <output-sockets>
  <socket client="system" exclusive="off" type="jack-audio" name="Input 1">
   <plug>capture_1</plug>
  </socket>
  <socket client="osu" exclusive="off" type="jack-audio" name="osu">
   <plug>capture_1</plug>
   <plug>capture_2</plug>
  </socket>
  <socket client="cloop" exclusive="off" type="jack-audio" name="ALSA Loopback Playback">
   <plug>capture_1</plug>
   <plug>capture_2</plug>
  </socket>
  <socket client="jack_rack" exclusive="off" type="jack-audio" name="JACK Rack">
   <plug>out_1</plug>
   <plug>out_2</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket client="osu audio" exclusive="off" type="jack-audio" name="OBS osu audio">
   <plug>in_1</plug>
   <plug>in_2</plug>
  </socket>
  <socket client="system" exclusive="off" type="jack-audio" name="System Output">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
  <socket client="jack_rack" exclusive="off" type="jack-audio" name="JACK Rack Input 1">
   <plug>in_1</plug>
  </socket>
  <socket client="jack_rack" exclusive="off" type="jack-audio" name="JACK Rack Input 2">
   <plug>in_2</plug>
  </socket>
  <socket client="PulseAudio JACK Source" exclusive="on" type="jack-audio" name="PulseAudio Source">
   <plug>front-left</plug>
   <plug>front-right</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable input="JACK Rack Input 1" type="jack-audio" output="Input 1"/>
  <cable input="JACK Rack Input 2" type="jack-audio" output="Input 1"/>
  <cable input="OBS osu audio" type="jack-audio" output="osu"/>
  <cable input="PulseAudio Source" type="jack-audio" output="JACK Rack"/>
 </cables>
</patchbay>

