<!DOCTYPE patchbay>
<patchbay name="scarlett" version="0.4.5">
 <output-sockets>
  <socket type="jack-audio" client="system" name="Input 1" exclusive="off">
   <plug>capture_1</plug>
  </socket>
  <socket type="jack-audio" client="osu" name="osu" exclusive="off">
   <plug>capture_1</plug>
   <plug>capture_2</plug>
  </socket>
  <socket type="jack-audio" client="cloop" name="ALSA Loopback Playback" exclusive="off">
   <plug>capture_1</plug>
   <plug>capture_2</plug>
  </socket>
  <socket type="jack-audio" client="jack_rack" name="JACK Rack" exclusive="off">
   <plug>out_1</plug>
   <plug>out_2</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket type="jack-audio" client="osu audio" name="OBS osu audio" exclusive="off">
   <plug>in_1</plug>
   <plug>in_2</plug>
  </socket>
  <socket type="jack-audio" client="system" name="System Output" exclusive="off">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
  <socket type="jack-audio" client="jack_rack" name="JACK Rack Input 1" exclusive="off">
   <plug>in_1</plug>
  </socket>
  <socket type="jack-audio" client="jack_rack" name="JACK Rack Input 2" exclusive="off">
   <plug>in_2</plug>
  </socket>
  <socket type="jack-audio" client="PulseAudio JACK Source" name="PulseAudio Source" exclusive="on">
   <plug>front-left</plug>
   <plug>front-right</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable input="JACK Rack Input 1" type="jack-audio" output="Input 1"/>
  <cable input="JACK Rack Input 2" type="jack-audio" output="Input 1"/>
  <cable input="OBS osu audio" type="jack-audio" output="osu"/>
  <cable input="System Output" type="jack-audio" output="ALSA Loopback Playback"/>
  <cable input="PulseAudio Source" type="jack-audio" output="JACK Rack"/>
 </cables>
</patchbay>

